  , "version"         : "0.3.0"
  , "homepage"        : "https://github.com/rvagg/node-bustermove"
  , "authors"         : [
        "Rod Vagg <rod@vagg.org> (https://github.com/rvagg)"
    ]
  , "keywords"        : [ "test", "buster", "tap" ]
  , "main"            : "./bustermove.js"
  , "repository"      : {
        "type"            : "git"
      , "url"             : "https://github.com/rvagg/node-bustermove.git"
    }
  , "dependencies" : {
